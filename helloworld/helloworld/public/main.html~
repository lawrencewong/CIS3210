    <!-- Init script -->
    <script type="text/javascript">	
	clippy.load('Links', function(agent) {
	    // Do anything with the loaded agent
	    agent.show();
	    $('#btnArtsy').on('click', function(event) {
	      agent.play('GetArtsy');
	    });
	    $('#btnCheck').on('click', function(event) {
	      agent.play('CheckingSomething');
	    });
	    $('#btnShred').on('click', function(event) {
	      agent.play('EmptyTrash');
	    });
	});
    </script>
  

